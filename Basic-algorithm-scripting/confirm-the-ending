function end(str, target) {
    var kara = str.split("");
    var ara = str.split(" ");
    var set = target.split("");
    


if (ara[ara.length-1] === target) {
        return true;
}

else if (set[set.length - 1] === kara[kara.length - 1]) {
    return true;    
}
    
else {
    return false;
}


    

    

}

console.log();


end("Walking on water and developing software from a specification are easy if both are frozen.", "specification");
